{
  "name": "health-check",
  "description": "Health check endpoints with database, memory, and Kubernetes probes",
  "category": "monitoring",
  "required": false,
  "deps": {
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/axios": "^3.0.0",
    "axios": "^1.4.0"
  },
  "devDeps": {},
  "env": [],
  "routes": [
    "/health",
    "/health/database",
    "/health/memory",
    "/health/storage"
  ],
  "dependsOn": ["config"],
  "optionalDeps": ["database"],
  "qualityChecklist": ["unit-tests", "integration-tests", "README", "openapi"],
  "filesPath": "files",
  "conflicts": [],
  "inject": {
    "src/app.module.ts": {
      "import": ["import { HealthModule } from '@modules/health/health.module';"],
      "register": ["HealthModule"]
    }
  },
  "fileStructure": [
    {
      "name": "health",
      "type": "folder",
      "path": "src/modules/health",
      "children": [
        { "name": "health.controller.ts", "type": "file" },
        { "name": "health.module.ts", "type": "file" }
      ]
    }
  ],
  "postInstall": []
}