{
  "name": "github-oauth",
  "description": "GitHub OAuth2 authentication integration",
  "category": "authentication",
  "required": false,
  "deps": {
    "passport-github2": "^0.1.12",
    "@nestjs/passport": "^10.0.0"
  },
  "devDeps": {},
  "env": [
    { "key": "GITHUB_CLIENT_ID", "required": true, "example": "abcdef1234567890abcd" },
    { "key": "GITHUB_CLIENT_SECRET", "required": true, "example": "abcdef1234567890abcdef1234567890abcdef12" },
    { "key": "GITHUB_CALLBACK_URL", "required": true, "example": "http://localhost:3000/auth/github/callback" }
  ],
  "routes": [
    "/auth/github",
    "/auth/github/callback"
  ],
  "dependsOn": ["config", "auth"],
  "optionalDeps": [],
  "qualityChecklist": ["unit-tests", "integration-tests", "README", "openapi"],
  "filesPath": "files",
  "conflicts": [],
  "inject": {
    "src/modules/auth/auth.module.ts": {
      "import": ["import { GitHubOAuthModule } from './github-oauth/github-oauth.module';"],
      "register": ["GitHubOAuthModule"]
    }
  },
  "fileStructure": [
    {
      "name": "github-oauth",
      "type": "folder",
      "path": "src/modules/auth/github-oauth",
      "children": [
        { "name": "github-oauth.module.ts", "type": "file" },
        { "name": "github-oauth.service.ts", "type": "file" },
        { "name": "github-oauth.controller.ts", "type": "file" },
        { "name": "github.strategy.ts", "type": "file" }
      ]
    }
  ],
  "postInstall": []
}